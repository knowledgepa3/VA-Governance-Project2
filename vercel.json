{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/portal.html" },
    { "source": "/demo", "destination": "/demo-landing.html" },
    { "source": "/try", "destination": "/demo-landing.html" },
    { "source": "/onboarding", "destination": "/onboarding.html" },
    { "source": "/setup", "destination": "/onboarding.html" },
    { "source": "/get-started", "destination": "/onboarding.html" },
    { "source": "/app", "destination": "/index-landing.html" },
    { "source": "/bd", "destination": "/bd.html" }
  ]
}
